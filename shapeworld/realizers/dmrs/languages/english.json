                "0.33": {"key": "at_most_a_third", "dmrs": "[quant]:_the_q --> [rstr]:pred x?[num=pl] <-1- _third_n_of x?[pers=3,num=sg] <-1- [body]:node; _at+most_x_deg e? ==> _a_q --> :_third_n_of"},
